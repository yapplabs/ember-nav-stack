{
  "name": "ember-nav-stack",
  "version": "4.3.3",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yapplabs/ember-nav-stack.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "release": "release-it",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-decorators": "6.1.1",
    "ember-math-helpers": "^3.0.0",
    "ember-truth-helpers": "^3.1.1",
    "hammerjs": "^2.0.8",
    "macro-decorators": "^0.1.2",
    "webpack": "5",
    "wobble": "^1.5.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.0",
    "@ember/test-waiters": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.24.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^11.0.1",
    "ember-route-action-helper": "^2.0.8",
    "ember-simulant-test-helpers": "^0.3.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^13.7.2",
    "release-it-lerna-changelog": "^2.4.0",
    "sass": "^1.66.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://yapplabs.github.io/ember-nav-stack"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.20.2",
    "yarn": "1.22.10"
  }
}
